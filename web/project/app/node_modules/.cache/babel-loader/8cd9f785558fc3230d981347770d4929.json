 tooltip: {\n            formatter(value) {\n              return value.data.name + \"<br/>\";\n            },\n\n            show: true\n          },\n          encode: {\n            value: 2\n          },\n          label: {\n            formatter: \"{b}\",\n            position: \"right\",\n            show: false\n          },\n          itemStyle: {\n            color: \"#0efacc\"\n          },\n          emphasis: {\n            label: {\n              show: false\n            }\n          }\n        }, {\n          name: \"显示所有机场\",\n          type: \"effectScatter\",\n          coordinateSystem: \"geo\",\n          data: data1,\n          textColor: \"#000\",\n          symbolSize: 8,\n          tooltip: {\n            formatter(value) {\n              return value.data.name + \"<br/>\" // + \"门店数：\" + value.data.value[2]\n              ;\n            },\n\n            show: true\n          },\n          encode: {\n            value: 2\n          },\n          showEffectOn: \"render\",\n          rippleEffect: {\n            brushType: \"stroke\",\n            color: \"#0efacc\",\n            period: 9,\n            scale: 5\n          },\n          hoverAnimation: true,\n          label: {\n            formatter: \"{b}\",\n            position: \"right\",\n            show: true\n          },\n          itemStyle: {\n            color: \"#0efacc\",\n            shadowBlur: 2,\n            shadowColor: \"#333\"\n          },\n          zlevel: 1\n        }]\n      };\n      myChart.setOption(option);\n      myChart.on(\"click\", params => {\n        console.log(params.seriesType);\n\n        if (params.seriesType == \"effectScatter\") {\n          if (this.thehint == \"请选择出发机场\") {\n            this.msg = params.name;\n            this.thehint = \"请选择目的机场\";\n          } else if (this.thehint == \"请选择目的机场\") {\n            this.emsg = params.name;\n            this.buttonShow = true;\n            this.thehint = \"请点击确认\";\n          } else if (this.thehint == \"请点击确认\") {\n            alert(\"请点击确定或取消\");\n          }\n        }\n\n        return;\n        this.$router.push({\n          path: \"/mapEchartProvince\",\n          query: {\n            provinceName: params.data.ename,\n            province: params.name\n          }\n        });\n      });\n      myChart.setOption(option);\n\n      window.onresize = () => {\n        myChart.resize();\n      };\n    },\n\n    getHint() {\n      var hintsGroup = [\"请选择出发机场\", \"请选择目的机场\", \"请点击确定\"];\n      return hintsGroup;\n    },\n\n    dealWithData() {\n      var geoCoordMap = [{\n        name: \"罗家机场\",\n        value: [117.18, 29.27, 20]\n      }, {\n        name: \"浦东机场\",\n        value: [121.80, 31.16, 60]\n      }, {\n        name: \"通辽机场\",\n        value: [122.256, 43.62, 20]\n      }, {\n        name: \"高崎国际机场\",\n        value: [118.03, 24.48, 10]\n      }, {\n        name: \"地窝堡国际机场\",\n        value: [88.31, 43.36, 10]\n      }, {\n        name: \"武宿国际机场\",\n        value: [112.487, 37.94, 11]\n      }, {\n        name: \"宝安机场\",\n        value: [113.88, 22.55, 12]\n      }, {\n        name: \"凤凰国际机场\",\n        value: [109.75, 18.4, 13]\n      }, {\n        name: \"流亭国际机场\",\n        value: [120.396, 36.9, 14]\n      }, {\n        name: \"遥墙国际机场\",\n        value: [116.75, 35.8, 15]\n      }, {\n        name: \"正定机场\",\n        value: [115.0, 40.03]\n      }, {\n        name: \"桃仙国际机场\",\n        value: [123.47, 41.8]\n      }, {\n        name: \"虹桥机场\",\n        value: [122.34, 30.196]\n      }, {\n        name: \"首都国际机场\",\n        value: [116.35, 39.04]\n      }, {\n        name: \"禄口国际机场\",\n        value: [119.89, 32.33]\n      }, {\n        name: \"两江国际机场\",\n        value: [110.3, 25.3]\n      }, {\n        name: \"长水国际机场\",\n        value: [101.82, 24.9]\n      }, {\n        name: \"太平机场\",\n        value: [126.96, 45.55]\n      }, {\n        name: \"白塔机场\",\n        value: [111.62, 40.8]\n      }, {\n        name: \"萧山国际机场\",\n        value: [120.21, 28.2]\n      }, {\n        name: \"长乐国际机场\",\n        value: [119.27, 26.04]\n      }, {\n        name: \"双流机场\",\n        value: [104.1, 30.66]\n      }, {\n        name: \"江北机场\",\n        value: [106.54, 29.4]\n      }, {\n        name: \"合肥机场\",\n        value: [117.3, 31.69]\n      }, {\n        name: \"新郑国际机场\",\n        value: [113.64, 34.72]\n      }, {\n        name: \"吐鲁番交河机场\",\n        value: [89.2, 40.94]\n      }, {\n        name: \"天河机场\",\n        value: [114.03, 30.58]\n      }];\n      return geoCoordMap;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmFA;AACA;AACAA;AACA;AACAC;AAEA;EACAC,YADA;;EAGAC;IACA;MACAC,YADA;MAGAC,iBAHA;MAIAC,QAJA;MAKAC,kBALA;MAMAC,UANA;MAOAC;IAPA;EAUA,CAdA;;EAgBAC;IACA;EACA,CAlBA;;EAmBAC;IACAC;MACA,oBACA;IACA,CAJA;;IAKAC;MACAC;MACA;MACA;MACA,kBACA,iBADA;MAEA;IACA,CAZA;;IAaAC;MACAD;MACAE;MACAA;MACAA;MACA;QACA;;QACA,kCAEA;MAEA,CANA;IAOA,CAzBA;;IA0BAC;MACA;QAAAC;MAAA;IACA,CA5BA;;IA6BAC;MACA;QAAAD;MAAA;IACA,CA/BA;;IAgCAE;MACA;MACA;MACA;MACA;MACA;QACAC;UACAC;QADA,CADA;QAIAC,0BAJA;QAKAC;UACAC,YADA;UAEAC,WAFA;UAEA;UACAC,UAHA;UAIAC,iBAJA;UAIA;UACAC;YACAC;cACAR,WADA;cACA;cACAS,cAFA;cAGAC;YAHA,CADA;YAMAC;cACAX;YADA;UANA,CALA;UAeAY;YACAJ;cACAK,oBADA;cAEAC,sBAFA;cAGAC,cAHA;cAGA;cACAC,aAJA;cAKAC,gBALA;cAMAC,gBANA;cAOAC;YAPA,CADA;YAUAR;cACAE,oBADA;cAEAK,gBAFA;cAGAD,gBAHA;cAIAD,aAJA;cAKAD,cALA;cAMAI;YANA;UAVA;QAfA,CALA;QAwCAC;UACAC,oBADA;UAEAC,aAFA;UAGAC,SAHA;UAIAC;QAJA,CAxCA;QA8CAC,SACA;UACAC,WADA;UAEAvB,YAFA;UAGAC,WAHA;UAIAC,UAJA;UAKAC,iBALA;UAMA;UACA;UACAqB,uBARA;UAQA;UACApB;YACA;YACAC;cACAE,gBADA;cAEAkB,gBAFA;cAGAC,mBAHA;cAIA7B;YAJA,CAFA;YAQAW;cACAX;YADA;UARA,CATA;UAqBAY;YACAJ;cACAK,oBADA;cAEAC,sBAFA;cAGAC;YAHA,CADA;YAMAJ;cACAE,oBADA;cAEAK,gBAFA;cAGAD,gBAHA;cAIAD,aAJA;cAKAD,cALA;cAMAI;YANA;UANA;QArBA,CADA,EAsCA;UACAvC,QADA;UAEA8C,eAFA;UAGAI,uBAHA;UAIAjD,eAJA;UAKAkD,gBALA;UAMAC,aANA;UAOAC,mBAPA;UAQAlC;YACA6B;cACA,OACAM,yBADA;YAGA,CALA;;YAMAlC;UANA,CARA;UAgBAmC;YACAD;UADA,CAhBA;UAmBA3B;YACAqB,gBADA;YAEAC,iBAFA;YAGA7B;UAHA,CAnBA;